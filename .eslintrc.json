{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/forbid-prop-types":"off",
        "no-console":"off",
        "max-len":"off",
        "no-return-assign":"off",
        "react/require-default-props":"off",
        "react/destructuring-assignment":"off"
    }
}